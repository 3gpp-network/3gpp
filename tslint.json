{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": [true, "elements", "members", "parameters", "statements"],
        "typedef": [true, "call-signature"],
        "triple-equals": true,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}

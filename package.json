{
  "name": "3gpp",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "antlr4ts": "antlr4ts -visitor -no-listener src/asn1/grammar/ASN_3gpp.g4 -o src/asn1/grammar",
    "antlr4ts-help": "antlr4ts",
    "extract": "node  dist/asn1/extractor",
    "prettier": "npx prettier@2.0.5 src --write",
    "test-parser": "mocha --enable-source-maps dist/asn1/parser --grep parse_all",
    "test-parser-36331": "mocha --enable-source-maps dist/asn1/parser --grep parse_36331",
    "test-parser-38331": "mocha --enable-source-maps dist/asn1/parser --grep parse_38331",
    "test-formatter": "mocha --enable-source-maps dist/asn1/formatter --grep format_all",
    "test-formatter-36331": "mocha --enable-source-maps dist/asn1/formatter --grep format_36331",
    "test-formatter-38331": "mocha --enable-source-maps dist/asn1/formatter --grep format_38331",
    "typedoc": "typedoc --out doc src",
    "typedoc-install": "npm i -g typedoc@0.16.11",
    "// Why a specific version of typedoc is required": "https://stackoverflow.com/a/60796968/3278063"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsongsong/3gpp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gsongsong/3gpp/issues"
  },
  "homepage": "https://github.com/gsongsong/3gpp#readme",
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "mocha": "^8.0.1"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3"
  }
}
